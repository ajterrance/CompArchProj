#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x5632033e40a0 .scope module, "Tmultiplier" "Tmultiplier" 2 1;
 .timescale 0 0;
v0x5632033f5bb0_0 .var "A", 3 0;
v0x5632033f5ca0_0 .var "B", 3 0;
v0x5632033f5d70_0 .net "Product", 7 0, v0x5632033f5a70_0;  1 drivers
S_0x5632033e4220 .scope module, "multiplier_inst" "Multiplier" 2 5, 3 1 0, S_0x5632033e40a0;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "A"
    .port_info 1 /INPUT 4 "B"
    .port_info 2 /OUTPUT 8 "Product"
v0x5632033ae0d0_0 .net "A", 3 0, v0x5632033f5bb0_0;  1 drivers
v0x5632033f5990_0 .net "B", 3 0, v0x5632033f5ca0_0;  1 drivers
v0x5632033f5a70_0 .var "Product", 7 0;
E_0x5632033e4760 .event edge, v0x5632033ae0d0_0, v0x5632033f5990_0;
    .scope S_0x5632033e4220;
T_0 ;
    %wait E_0x5632033e4760;
    %load/vec4 v0x5632033ae0d0_0;
    %pad/u 8;
    %load/vec4 v0x5632033f5990_0;
    %pad/u 8;
    %mul;
    %store/vec4 v0x5632033f5a70_0, 0, 8;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5632033e40a0;
T_1 ;
    %vpi_call 2 13 "$monitor", "At time %t: Product = %b, A = %b, B = %b", $time, v0x5632033f5bb0_0, v0x5632033f5ca0_0, v0x5632033f5d70_0 {0 0 0};
    %pushi/vec4 5, 0, 4;
    %store/vec4 v0x5632033f5bb0_0, 0, 4;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x5632033f5ca0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x5632033f5bb0_0, 0, 4;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x5632033f5ca0_0, 0, 4;
    %delay 10, 0;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x5632033f5bb0_0, 0, 4;
    %pushi/vec4 7, 0, 4;
    %store/vec4 v0x5632033f5ca0_0, 0, 4;
    %delay 10, 0;
    %end;
    .thread T_1;
    .scope S_0x5632033e40a0;
T_2 ;
    %vpi_call 2 34 "$dumpfile", "Multiplier.vcd" {0 0 0};
    %vpi_call 2 35 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5632033e40a0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "Tmultiplier.v";
    "Multiplier.v";
